# WealthSage Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file with real values!

# Tavily API Key (Required)
# Get your API key from: https://tavily.com/
TAVILY_API_KEY=your_tavily_api_key_here

# Optional: Database URL (if using external database)
# DATABASE_URL=postgresql://username:password@localhost:5432/wealthsage

# Optional: Redis URL (if using Redis for caching)
# REDIS_URL=redis://localhost:6379

# Optional: Environment
# ENVIRONMENT=development

# Optional: Debug mode
# DEBUG=true

# Optional: CORS origins (comma-separated)
# CORS_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:5174

# Optional: Log level
# LOG_LEVEL=INFO
